{
    "plugins": [
        "jest"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "error",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    },
    "extends": [
        "eslint:recommended",
        "plugin:sonarjs/recommended",
        "plugin:react/recommended",
        "plugin:jest/recommended",
        "plugin:jest/style"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}